<%= simple_form_for(@auction, html: { class: 'form-horizontal' }) do |f| %>
  <%= f.input :title %>
  <%= f.input :details %>
  <%= f.input :reserve %>
  <div class="form-group">
    <%= f.label :end_date %>
    <%= f.date_field :end_date %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>